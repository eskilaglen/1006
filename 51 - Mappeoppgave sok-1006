   "source": [
    "import numpy as np\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "#definerer tilbud og etterspørsel (hentet fra forelesning 5)\n",
    "def tilbud(p,t):\n",
    "    return -100+4.3*(p-t)\n",
    "\n",
    "def etterspørsel(p,T):\n",
    "    return 500 - 3.2*(p+T)\n",
    "\n",
    "\n",
    "p_num=np.linspace(0.1,100,100)"
   ]
  },
